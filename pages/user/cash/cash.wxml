<view class='pa-30'>
  <view class='panel ra-10 shadow'>
    <view class='pa-20'>
      <view class='panel-start item-center'>
        <view class='fo-28'>你的积分</view>
        <view class='fo-50 ' style='margin-left:100rpx'>{{showFee}} <text class='fo-26'>分</text></view>
      </view>
      <view class='fo-28 ma-t30'>换取奖品</view>
      <view class='pa-b10 pa-t30 border-b'>
        <view class='panel-between item-center'>
          <view class='fo-50'>￥</view>
          <input placeholder='你想用多少积分换区奖品' type='digit' bindinput='cashInput' value='{{cashFee}}' placeholder-class='fo-28'></input>
          <view class='fo-t fo-28' bindtap='cashAll'>全部</view>
        </view>
      </view>
      <view class='fo-26 fo-g pa-t30'>请联系客服换取奖品</view>
      <button type='primary' class='ma-t30'  bindtap='cashIt'>换取</button>
    </view>
  </view>
</view>
<!-- <view class='pa-30 text-center fo-30'>提现申请将在1-3个工作日内到账</view> -->
<view class=" pa-30 panel-center">
  <button class='filter-btn fo-30 fo-t'  open-type='contact'>联系客服</button>
</view>